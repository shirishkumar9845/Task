<!DOCTYPE html>
<html lang="en">
<head>
  <title>GooglePlaceSearch</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>

<body>

  <div class="container-fluid">

    <div class="navbar">
      <div class="navbar-inner">
        <ul class="nav">
          <li class="<%= active_for('index') %>"><%= link_to "Stored Locations", locations_path %></li>
          <li class="divider-vertical"></li>
        </ul>
      </div>
    </div>

    <div class="row-fluid">
      <div class='flash'>
        <%= bootstap_flash %>
      </div>
      <%= yield %>
    </div>
  </div>

</body>
</html>
